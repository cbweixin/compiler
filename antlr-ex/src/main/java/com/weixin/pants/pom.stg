groupIdTemplate(id) ::= <<
\<groupId><id>\</groupId>
>>

artifactIdTemplate(id,condition) ::= <%
<if(condition)>\<artifactId><id>_scala_2.11\</artifactId>
<else>\<artifactId><id>\</artifactId>
<endif>
%>

versionTemplate(ver) ::= <<
\<version><ver>\</version>
>>


entryTemplate(tag,entry) ::= <<
\<<tag>\>
    <entry>
\</<tag>\>
>>

entriesTemplate(tag,value) ::= <<
\<<tag>\>
    <value>
\</<tag>\>
>>





